        "bitcore-lib": "8.25.10",
        "bitcore-lib-cash": "8.25.10"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": false,
        "tabWidth": 4,
        "trailingComma": "all",
        "endOfLine": "lf",
        "arrowParens": "always"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    }
}
