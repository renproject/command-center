        "@sentry/integrations": "^6.11.0",
        "@types/bs58": "^4.0.1",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.7.1",
        "@types/node-fetch": "^2.5.12",
        "@types/query-string": "^6.3.0",
        "@types/react": "^17.0.19",
        "@types/react-circular-progressbar": "^1.1.0",
